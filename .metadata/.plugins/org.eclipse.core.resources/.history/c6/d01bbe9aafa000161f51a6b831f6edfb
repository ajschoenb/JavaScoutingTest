package frc;

public class Config {
	private static String[][] sidebarConfig = {
			{"H"},
			{"H", "X"},
			{"L"},
			{"L", "X"},
			{"\u0394"},
			{"\u2207"}};
	private static String fieldURL = null;
	
	public static void setupVars() {
		sidebarConfig[0][0] = "H";
		sidebarConfig[1][0] = "H";
		sidebarConfig[1][1] = "X";
		sidebarConfig[2][0] = "L";
		sidebarConfig[3][0] = "L";
		sidebarConfig[3][1] = "X";
		sidebarConfig[4][0] = "\u0394";
		sidebarConfig[5][0] = "\u2207";
		fieldURL = "file:img/field.png";
	}
	
	public static String getFieldURL() {
		return fieldURL;
	}
	
	public static String[][] getSidebarConfig() {
		return sidebarConfig;
	}
}
